package com.lti.model;

import java.time.LocalTime;

import javax.persistence.Entity;
import javax.persistence.Table;

@Entity
@Table(name="TBL_Stop")
public class Stop {
	@Id
	private int stopno;
	private String name;
	private LocalTime arrivalTime;
	private LocalTime departureTime;
	
	private Bus bus;

}
